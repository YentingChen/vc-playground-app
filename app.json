{
  "expo": {
    "name": "vc-playground-app",
    "slug": "vc-app-playground",
    "owner": "retailmenotuk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon-playground.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "uk.co.vouchercodes.debug",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.INTERNET"
      ],
      "package": "co.uk.vouchercodes.playground"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "@braze/expo-plugin",
        {
          "androidApiKey": "7ab31baa-4f04-42ae-b751-f271eb9166ea",
          "iosApiKey": "98edd563-8606-4705-b19c-8156d66fcd2b",
          "baseUrl": "sdk.fra-01.braze.eu",
          "enableFirebaseCloudMessaging": true,
          "firebaseCloudMessagingSenderId": "860544725534",
          "androidHandlePushDeepLinksAutomatically": true,
          "iosRequestPushPermissionsAutomatically": true,
          "enableBrazeIosPush": true,
          "enableBrazeIosPushStories": true,
          "enableBrazeIosRichPush": true,
          "iosPushStoryAppGroup": "group.uk.co.vouchercodes.debug"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "newArchEnabled": true
          },
          "ios": {
            "deploymentTarget": "14.0",
            "newArchEnabled": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "390048b7-013a-4b93-b2cb-45503b0df5b2",
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "BrazeExpoRichPush",
                  "bundleIdentifier": "uk.co.vouchercodes.debug.BrazeExpoRichPush",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.uk.co.vouchercodes.debug"
                    ]
                  }
                },
                {
                  "targetName": "BrazeExpoPushStories",
                  "bundleIdentifier": "uk.co.vouchercodes.debug.BrazeExpoPushStories",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.uk.co.vouchercodes.debug"
                    ]
                  }
                }
              ]
            }
          }
        }
      }
    }
  }
}
