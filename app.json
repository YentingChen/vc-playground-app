{
  "expo": {
    "name": "vc-playground-app",
    "slug": "vc-app-playground",
    "owner": "retailmenotuk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon-playground.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "co.uk.vouchercodes.playground"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.INTERNET"
      ],
      "package": "co.uk.vouchercodes.playground"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "390048b7-013a-4b93-b2cb-45503b0df5b2"
      }
    }
  }
}
